<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jmdb.bookmein2" version="2.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="browser-package" value="BarcodeScanner" />
    </feature>
    <name>BookMeIn2</name>
    <description>
        Register attendees at conferences and events.
    </description>
    <author email="james@jmdbsoftware.com" href="http://jmdbsoftware.com">
				James Rooney
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="phonegap-version" value="cli-9.0.0" />
</widget>
